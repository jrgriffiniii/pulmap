<header role="banner">
  <div class="l-branding__pul">
    <div class="container">
      <%= link_to root_path, class: "logo__pul" do %>
        <%= image_tag("library_full_logo.svg", class: "logo-svg") %>
        <%= image_tag("library_full_logo.png", class: "logo-no-svg") %>
      <% end %>
            <div class="pull-right menu--level-1 col-xs-12 col-sm-6">
        <ul>
          <li><a href="http://library.princeton.edu/help/contact-us">Contact Us</a></li>
          <%= render :partial=>'/account' %>
        </ul>
      </div>
    </div>
  </div>
  <div class="home-secondary-navbar navbar">
    <div class="container" role="navigation">
      <div class="col-md-6 logo">
      <%= link_to root_path, class: "logo__pul" do %>
        <%= image_tag 'lime.png', {:alt => 'GeoBlacklight', :style => 'height: 25px;', :class => 'pulmap-brand'} %>
        <% end %>
      </div>
      <div class="col-md-6 util-links">
        <%= render :partial=>'/user_util_links' -%>
      </div>
    </div>
  </div>

  <% if home_layout? %>
    <%# if there are no input/search related params, display the "secondary home" partial -%>
    <%= render 'shared/header_secondary_home' %>
  <% elsif item_layout? %>
    <%= render 'shared/header_secondary_index' %>
  <% else %>
    <%= render 'shared/header_secondary_filters' %>
  <% end %>

</header>